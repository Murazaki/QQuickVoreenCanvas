 *                                                                    *
 **********************************************************************/

#include "modules/mod_sampler2d.frag"

uniform sampler2D colorTex_;
uniform sampler2D depthTex_;
uniform TextureParameters texParams_;

float numLevels = 10.0;
float contrast = 2.5;

void main() {
    vec2 p = gl_FragCoord.xy * screenDimRCP_;
    vec4 fragColor = textureLookup2Dnormalized(colorTex_, texParams_, p);
	
    float step = 1.0 / numLevels;
    if (fragColor.a > 0.0) {
        float prob = fragColor.a;
        float quantValue;
        if (prob <= 0.5) {
            quantValue = pow(prob*2.0, contrast) / 2.0;
        }
        else {
			quantValue = 1.0 - (pow((1.0 - prob) * 2.0, contrast) / 2.0);
        }
 
        quantValue -= mod(quantValue, step);
        quantValue = clamp(quantValue+step/2.0, 0.0, 1.0);
 
	    if (quantValue <= 0.5) {
            quantValue = pow(quantValue*2.0, 1.0/contrast) / 2.0;
        }
        else {
			quantValue = 1.0 - (pow((1.0 - quantValue) * 2.0, 1.0/contrast) / 2.0);
        }

        fragColor.xyzw = vec4(quantValue);
	}

    gl_FragColor = fragColor;
    gl_FragDepth = textureLookup2Dnormalized(depthTex_, texParams_, p).z;
}
